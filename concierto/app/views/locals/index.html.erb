<h1>Listing locals</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Direccion</th>
    <th>Distrito</th>
    <th>Direccgoogle</th>
    <th>Telfijo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @locals.each do |local| %>
  <tr>
    <td><%= local.nombre %></td>
    <td><%= local.direccion %></td>
    <td><%= local.distrito_id %></td>
    <td><%= local.direccGoogle %></td>
    <td><%= local.telFijo %></td>
    <td><%= link_to 'Show', local %></td>
    <td><%= link_to 'Edit', edit_local_path(local) %></td>
    <td><%= link_to 'Destroy', local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

